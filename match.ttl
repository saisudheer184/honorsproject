@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# --- Vocabulary/Schema Definitions ---
# Defining the key classes for clarity in the graph
vocab:Match a rdfs:Class ; rdfs:label "Match" .
vocab:Team a rdfs:Class ; rdfs:label "Team" .
vocab:Venue a rdfs:Class ; rdfs:label "Venue" .
vocab:Player a rdfs:Class ; rdfs:label "Player" .
vocab:BattingPerformance a rdfs:Class ; rdfs:label "Batting Performance" .
vocab:BowlingPerformance a rdfs:Class ; rdfs:label "Bowling Performance" .

# --- Match, Venue, and Innings Data ---
ex:Chepauk_Stadium a vocab:Venue ;
    vocab:name "MA Chidambaram Stadium, Chennai" ;
    vocab:location "Chennai" .

ex:Team_CSK a vocab:Team ; vocab:teamName "Chennai Super Kings" ; vocab:shortName "CSK" .
ex:Team_RCB a vocab:Team ; vocab:teamName "Royal Challengers Bengaluru" ; vocab:shortName "RCB" .

ex:Match_IPL2024_01 a vocab:Match ;
    vocab:matchID "IPL2024_01"^^xsd:string ;
    vocab:date "2024-03-22"^^xsd:date ;
    vocab:venue ex:Chepauk_Stadium ;
    vocab:teamA ex:Team_CSK ;
    vocab:teamB ex:Team_RCB ;
    vocab:winner ex:Team_CSK ;
    vocab:totalRuns_TeamA 176 ;
    vocab:totalRuns_TeamB 173 .

